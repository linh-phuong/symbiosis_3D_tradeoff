	journal = {Med. Vet. Entomol.},
	keywords = {Anopheles gambiae,Electroantennogram,Host-seeking behaviour,Human odours,Malaria mosquito,Olfaction},
	number = {3},
	pages = {280--287},
	pmid = {17044878},
	title = {{Interindividual variation in the attractiveness of human odours to the malaria mosquito Anopheles gambiae s. s.}},
