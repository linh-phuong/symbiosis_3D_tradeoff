	publisher = {Elsevier B.V.},
	title = {{Occurrence of natural {\textless}i{\textgreater}Anopheles arabiensis{\textless}/i{\textgreater} swarms in an urban area of Bobo-Dioulasso city, Burkina Faso, West Africa}},
