	keywords = {an,anopheles arabiensis,burkina faso,carbon dioxide,culex,funestus,gambiae,host-seeking behaviour,malaria,mansonia uniformis,odour-baited traps,pharoensis,quinquefasciatus,sudan savanna,vectors,west africa},
