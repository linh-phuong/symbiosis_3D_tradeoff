@article{Oliver2003,
	abstract = {Symbiotic relationships between animals and microorganisms are common in nature, yet the factors controlling the abundance and distributions of symbionts are mostly unknown. Aphids have an obligate association with the bacterium Buchnera aphidicola (the primary symbiont) that has been shown to contribute directly to aphid fitness. In addition, aphids sometimes harbor other vertically transmitted bacteria (secondary symbionts), for which few benefits of infection have been previously documented. We carried out experiments to determine the consequences of these facultative symbioses in Acyrthosiphon pisum (the pea aphid) for vulnerability of the aphid host to a hymenopteran parasitoid, Aphidius ervi, a major natural enemy in field populations. Our results show that, in a controlled genetic background, infection confers resistance to parasitoid attack by causing high mortality of developing parasitoid larvae. Compared with uninfected controls, experimentally infected aphids were as likely to be attacked by ovipositing parasitoids but less likely to support parasitoid development. This strong interaction between a symbiotic bacterium and a host natural enemy provides a mechanism for the persistence and spread of symbiotic bacteria.},
	archiveprefix = {arXiv},
	arxivid = {1011.1669v3},
	author = {Oliver, K. M. and Russell, J. A. and Moran, N. A. and Hunter, M. S.},
	doi = {10.1073/pnas.0335320100},
	eprint = {1011.1669v3},
	file = {:Users/Linh-phuong/Library/Application Support/Mendeley Desktop/Downloaded/Oliver et al. - Unknown - Facultative bacterial symbionts in aphids confer resistance to parasitic wasps.pdf:pdf},
	isbn = {0027-8424},
	issn = {0027-8424},
	journal = {Proc. Natl. Acad. Sci.},
	number = {4},
	pages = {1803--1807},
	pmid = {12563031},
	title = {{Facultative bacterial symbionts in aphids confer resistance to parasitic wasps}},
	url = {http://www.pnas.org/cgi/doi/10.1073/pnas.0335320100},
	volume = {100},
	year = {2003},
	bdsk-url-1 = {http://www.pnas.org/cgi/doi/10.1073/pnas.0335320100},
	bdsk-url-2 = {https://doi.org/10.1073/pnas.0335320100}}

@article{Wernegreen2002,
	abstract = {More than 10{\%} of insect species, in several taxonomic orders, depend on OBLIGATE BACTERIAL MUTUALISTS for their viability and reproduction 1--5 . These intracellular asso-ciates are called 'primary' (P)-endosymbionts, as they are required for the survival and fecundity of the host. In addition to their ecological importance, P-endosymbionts are ideal systems in which to study the changes in rates and patterns of bacterial evolu-tion that have occurred during the transition from a free-living to an intracellular lifestyle. For example, P-endosymbionts include the smallest, most (A+T)-rich bacterial genomes discovered so far and are quite distinct from the genomes of their free-living close bacterial relatives, which are moderately sized and have moderate percentage G+C contents. Consequently, endosymbionts offer a unique oppor-tunity to examine the molecular and evolutionary forces that drive changes in base composition and genome size in bacteria generally. P-endosymbionts of insects were first characterized through microscopy (for example, see REF. 1); however, because obligate symbionts cannot be readily cultured apart from the host, they have proved difficult to study by using traditional genetic or physiological tech-niques. During the past decade, our understanding of insect endosymbionts has improved markedly through intensive molecular analysis. DNA-sequencing projects have illustrated the mechanisms and consequences of severe genome size reduction among obligate bacterial associates compared with their free-living relatives. Here, I review the insights that molecular and compu-tational approaches have provided into the diversity and significance of insect mutualists, the mechanisms that have led to the reduction in their genome sizes, and the evolutionary implications thereof. I also high-light the parallels that can be drawn between mutual-ists and obligate pathogens, and the more general evolutionary forces that act on the bacterial inhabi-tants of eukaryotic cells.},
	author = {Wernegreen, Jennifer J},
	doi = {10.1038/nrg931},
	file = {:Users/Linh-phuong/Library/Application Support/Mendeley Desktop/Downloaded/Wernegreen - 2002 - Genome evolution in bacterial endosymbionts of insects.pdf:pdf},
	journal = {Nat. Rev. Genet.},
	title = {{Genome evolution in bacterial endosymbionts of insects}},
	url = {http://content.ebscohost.com/ContentServer.asp?EbscoContent=dGJyMNXb4kSep7U4yNfsOLCmr1Cep65SsKe4S7SWxWXS{\&}ContentCustomer=dGJyMOzpsE2xqK5IuePfgeyx9Yvf5ucA{\&}T=P{\&}P=AN{\&}S=R{\&}D=a9h{\&}K=7746204},
	volume = {3},
