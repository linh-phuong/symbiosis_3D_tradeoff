	keywords = {Animals,Biological Evolution,Ecosystem,Genetic,Models,Plants,Plants: genetics,Reproducibility of Results,Reproduction,Reproduction: genetics,Selection,Symbiosis,Symbiosis: genetics},
	number = {3},
	pages = {309--23},
	pmid = {10366555},
	title = {{Evolution of mutualistic symbiosis without vertical transmission.}},
	url = {http://www.sciencedirect.com/science/article/pii/S0040580998914071},
	volume = {55},
	year = {1999},
	bdsk-url-1 = {http://www.sciencedirect.com/science/article/pii/S0040580998914071},
