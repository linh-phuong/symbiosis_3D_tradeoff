	month = {jul},
	number = {7},
	pages = {3687--3694},
	pmid = {12839733},
	publisher = {American Society for Microbiology},
	title = {{Role of nonhost environments in the lifestyles of Salmonella and Escherichia coli}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/12839733 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC165204},
	volume = {69},
	year = {2003},
	bdsk-url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/12839733%20http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC165204},
