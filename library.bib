	issn = {14712970},
	number = {1378},
	pages = {9--15},
	pmid = {9061958},
	title = {{Virulence, parasite mode of transmission, and host fluctuating asymmetry}},
	url = {https://royalsocietypublishing.org/doi/pdf/10.1098/rspb.1997.0002},
	volume = {264},
	year = {1997},
	bdsk-url-1 = {https://royalsocietypublishing.org/doi/pdf/10.1098/rspb.1997.0002},
