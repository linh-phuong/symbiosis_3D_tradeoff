@article{Poisot2013,
	abstract = {Network theory is gaining momentum as a descriptive tool in community ecology. Because organisms with the same lifestyle can still exhibit ecological differences, it is crucial to determine the scale at which networks should be described. Here we show that networks of hosts (mammals) and parasites (ectoparasitic gamasid mites) differ when either facultative or obligatory parasites only are considered. More importantly, the structure of these networks is opposed, with obligatory parasites networks being more modular, and facultative parasites networks being more nested. Our results have consequences for the way we define which species to include in ecological networks, which we discuss in the light of community ecology and epidemiology.},
	author = {Poisot, Timoth{\'{e}}e and Stanko, Michal and Miklisov{\'{a}}, Dana and Morand, Serge},
	doi = {10.1017/S0031182013000851},
	file = {:Users/Linh-phuong/Library/Application Support/Mendeley Desktop/Downloaded/Poisot et al. - 2013 - Facultative and obligate parasite communities exhibit different network properties.pdf:pdf},
	issn = {1469-8161},
	journal = {Parasitology},
	number = {11},
	pages = {1340--5},
	pmid = {23920022},
	title = {{Facultative and obligate parasite communities exhibit different network properties.}},
