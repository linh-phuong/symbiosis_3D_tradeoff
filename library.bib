@article{Loeuille2014,
	abstract = {Local negative feedbacks occur when the occupation of a site by a species decreases the subsequent fitness of related individuals compared to potential competitors. Such negative feedbacks can enhance diversity by changing the spatial structure of the environment. The conditions, however, involve dispersive, environmental and evolutionary processes in complex interactive ways. We introduce a model that accounts for four mechanisms: colonisation-competition-extinction ecological dynamics, evolutionary dynamics, local negative feedbacks and environmental averaging. Three qualitatively distinct dynamics are possible, one dominated by specialists, another dominated by generalists and an intermediate situation exhibiting taxon cycles. We discuss how metacommunity diversity, macro-ecological patterns and environmental patterning are linked to the three qualitative dynamics. The model provides classical shapes for morph-abundance distributions, or diversity-area relationships. Diversity can be high when specialists dominate or when taxon cycles happen. Finally, local negative feedbacks often yield fine-grain environments for taxon cycle dynamics and coarse-grain environments when generalists dominate.},
	author = {Loeuille, Nicolas and Leibold, Mathew A.},
	doi = {10.1111/ele.12258},
	file = {:Users/Linh-phuong/Documents/PhD/PostDoc/Niche{\_}Construction/LoueilleandLeibold2014.pdf:pdf},
	issn = {14610248},
	journal = {Ecol. Lett.},
	keywords = {Eco-evolutionary feedbacks,Environmental grain,Janzen-Connell hypothesis,Metacommunity,Metaecosystem,Niche construction,Species abundance distribution,Species area relationship,Taxon cycle},
	number = {5},
	pages = {563--573},
	title = {{Effects of local negative feedbacks on the evolution of species within metacommunities}},
	volume = {17},
	year = {2014},
	bdsk-url-1 = {https://doi.org/10.1111/ele.12258}}

