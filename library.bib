	keywords = {ecology,epidemiology,health and disease and},
	number = {1821},
	pages = {2430},
	pmid = {26674956},
	title = {{Evidence for carry-over effects of predator exposure on pathogen transmission potential}},
	url = {http://rspb.royalsocietypublishing.org.libproxy.temple.edu/content/282/1821/20152430},
	volume = {282},
	year = {2015},
	bdsk-url-1 = {http://rspb.royalsocietypublishing.org.libproxy.temple.edu/content/282/1821/20152430},
