	number = {6},
	pages = {765--778},
	title = {{Trade-off geometries and frequency-dependent selection}},
	url = {http://www.journals.uchicago.edu.insb.bib.cnrs.fr/doi/pdfplus/10.1086/424762},
	volume = {164},
	year = {2004},
