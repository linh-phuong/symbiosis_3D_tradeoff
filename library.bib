	journal = {Parasitology},
	keywords = {Nematodes,adaptation,convergence,genomes,plant parasitism},
	pages = {S71--S84},
	pmid = {25656361},
	title = {{Signatures of adaptation to plant parasitism in nematode genomes}},
	url = {https://www-cambridge-org.insb.bib.cnrs.fr/core/services/aop-cambridge-core/content/view/A84B46DCDFBFC2999215D0973A32008F/S0031182013002163a.pdf/signatures{\_}of{\_}adaptation{\_}to{\_}plant{\_}parasitism{\_}in{\_}nematode{\_}genomes.pdf},
