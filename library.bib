@article{Mideo2008,
	abstract = {Nested models (also called embedded models) explicitly link dynamical processes that occur at different scales. Recently there has been considerable interest in linking within- and between-host levels of disease dynamics in the study of pathogen evolution. Here we review the extent to which these nested models have increased our understanding of pathogen evolution. We suggest that, although such models have been useful for determining the nature of tradeoffs between epidemiological parameters and for evaluating the consequences of conflicting selection pressures at different scales, the vast majority of previous results could likely have been obtained without the use of nested models per se. Nevertheless, these models have proven very useful through their highlighting of the importance of within-host disease dynamics on pathogen evolution. ?? 2008 Elsevier Ltd. All rights reserved.},
	author = {Mideo, Nicole and Alizon, Samuel and Day, Troy},
	doi = {10.1016/j.tree.2008.05.009},
	file = {:Users/Linh-phuong/Documents/To be read when free/MideoETAL{\_}2008.pdf:pdf},
	isbn = {0169-5347 (Print)$\backslash$r0169-5347},
	issn = {01695347},
	journal = {Trends Ecol. Evol.},
	number = {9},
	pages = {511--517},
	pmid = {18657880},
	title = {{Linking within- and between-host dynamics in the evolutionary epidemiology of infectious diseases}},
	volume = {23},
	year = {2008},
	bdsk-url-1 = {https://doi.org/10.1016/j.tree.2008.05.009}}

@article{Best2017,
	abstract = {Fluctuating selection driven by coevolution between hosts and parasites is important for the generation of host and parasite diversity across space and time. Theory has focused primarily on infection genetics, with highly specific `matching-allele' frameworks more likely to generate fluctuating selection dynamics (FSD) than `gene-for-gene' (generalist--specialist) frameworks. However, the environment, ecological feedbacks and life-history characteristics may all play a role in determining when FSD occurs. Here, we develop eco-evolutionary models with explicit ecological dynamics to explore the ecological, epidemiological and host life-history drivers of FSD. Our key result is to demonstrate for the first time, to our knowledge, that specificity between hosts and parasites is not required to generate FSD. Furthermore, highly specific host--parasite interactions produce unstable, less robust stochastic fluctuations in contrast to interactions that lack specificity altogether or those that vary from generalist to specialist, which produce predictable limit cycles. Given the ubiquity of ecological feedbacks and the variation in the nature of specificity in host--parasite interactions, our work emphasizes the underestimated potential for host--parasite coevolution to generate fluctuating selection.},
	author = {Best, Alex and Ashby, Ben and White, Andy and Bowers, Roger and Buckling, Angus and Koskella, Britt and Boots, Mike},
	doi = {10.1098/rspb.2017.1615},
	file = {:Users/Linh-phuong/Library/Application Support/Mendeley Desktop/Downloaded/Best et al. - 2017 - Host-parasite fluctuating selection in the absence of specificity.pdf:pdf},
	isbn = {0000000337},
	issn = {14712954},
	journal = {Proc. R. Soc. B Biol. Sci.},
	keywords = {Coevolution,Fluctuating selection,Infectious disease,Specificity},
	month = {nov},
	number = {1866},
	pmid = {29093222},
	publisher = {The Royal Society},
	title = {{Host-parasite fluctuating selection in the absence of specificity}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/29093222 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5698645},
	volume = {284},
