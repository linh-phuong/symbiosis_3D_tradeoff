	issn = {1477-9145},
	journal = {J. Exp. Biol.},
	keywords = {aedes aegypti,attraction,carbon dioxide,host-finding,lactic acid,mosquito,odour plume,regardless of plume type,the,upwind flight,were significantly lower},
	pages = {1639--1648},
	pmid = {10333509},
	title = {{Influence of odour plume structure on upwind flight of mosquitoes towards hosts}},
