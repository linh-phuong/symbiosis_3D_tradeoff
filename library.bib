	number = {1},
	pages = {161--181},
	pmid = {15487934},
	title = {{Stationary-Phase Physiology}},
	url = {http://dx.doi.org/10.1146/annurev.micro.58.030603.123818{\%}5Cnhttp://www.annualreviews.org/doi/pdf/10.1146/annurev.micro.58.030603.123818},
