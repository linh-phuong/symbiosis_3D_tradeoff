	pages = {163--173},
	publisher = {Narnia},
	title = {{What drives bacteria to produce a biofilm?}},
	url = {https://academic.oup.com/femsle/article-lookup/doi/10.1111/j.1574-6968.2004.tb09643.x},
	volume = {236},
