@article{Lion2018a,
	abstract = {A widespread tenet is that evolution of pathogens maximizes their basic reproduction number, R0. The breakdown of this principle is typically discussed as expection. Here, we argue that a radically different stance is needed, based on evolutionarily stable strategy (ESS) arguments that take account of the 'dimension of the environmental feedback loop.' The R0 maximisation paradigm requires this feedback loop to be one-dimensional, which notably excludes pathogen diversification. By contrast, almost all realistic ecological ingredients of host-pathogen interactions (density-dependent mortality, multiple infections, limited cross-immunity, multiple transmission routes, host heterogeneity, and spatial structure) will lead to multidimensional feedbacks.},
	author = {Lion, S{\'{e}}bastien and Metz, Johan A.J.},
	doi = {10.1016/j.tree.2018.02.004},
	file = {:Users/Linh-phuong/Library/Application Support/Mendeley Desktop/Downloaded/Lion, Metz - 2018 - Beyond R 0 Maximisation On Pathogen Evolution and Environmental Dimensions.pdf:pdf},
	issn = {01695347},
	journal = {Trends Ecol. Evol.},
	keywords = {basic reproduction ratio,enviromental feedback,evolutionary optimisation,pathogen evolution,virulence},
	title = {{Beyond R 0 Maximisation: On Pathogen Evolution and Environmental Dimensions}},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S0169534718300363},
	year = {2018},
