	issn = {00199567},
	journal = {Infect. Immun.},
	number = {11},
	pages = {4708--4715},
	pmid = {21844236},
	title = {{Infection intensity-dependent responses of Anopheles gambiae to the African malaria parasite {\textless}i{\textgreater}Plasmodium falciparum{\textless}/i{\textgreater}}},
