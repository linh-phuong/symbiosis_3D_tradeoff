	journal = {Proc. Natl. Acad. Sci. U. S. A.},
	keywords = {Animals,Anopheles,Behavior, Animal,Female,Insect Vectors,Malaria,Mice,Odors,Plasmodium chabaudi},
	number = {30},
	pages = {11079--84},
	pmid = {24982164},
