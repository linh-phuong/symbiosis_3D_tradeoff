	title = {{Advances in methods for colour marking of mosquitoes.}},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3708792{\&}tool=pmcentrez{\&}rendertype=abstract},
	volume = {6},
