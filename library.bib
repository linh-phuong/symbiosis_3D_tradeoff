	issn = {1475-2875},
	journal = {Malar. J.},
	keywords = {*Disease Vectors,Animal Feed,Animals,Anopheles gambiae/*parasitology/*physiology,Blood/parasitology,Entomology/*methods,Female,Humans,Plasmodium falciparum/*growth {\&} development},
	number = {1},
