	journal = {Parasit. Vectors},
	keywords = {Animals,Anopheles gambiae,Anopheles gambiae: chemistry,Anopheles gambiae: physiology,Cattle,Feeding Behavior,Female,Fluorescent Dyes,Fluorescent Dyes: chemistry,Humans,Insect Vectors,Insect Vectors: chemistry,Insect Vectors: physiology,Male,Mosquito Control,Staining and Labeling,Staining and Labeling: methods},
	number = {1},
