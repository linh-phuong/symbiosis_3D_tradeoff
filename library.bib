	keywords = {Holobiont,Holobiont model,Hologenome,Level of sele,holobiont,holobiont model,hologenome,level of selection,microbiome,microbiota,symbiont},
	number = {0},
	pages = {0},
	publisher = {Springer Netherlands},
	title = {{Holobionts as Units of Selection and a Model of Their Population Dynamics and Evolution}},
	url = {http://link.springer.com/10.1007/s13752-017-0287-1},
	volume = {0},
	year = {2017},
	bdsk-url-1 = {http://link.springer.com/10.1007/s13752-017-0287-1},
