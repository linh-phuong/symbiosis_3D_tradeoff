@article{Gilchrist2002,
	abstract = {In this study we introduce a mechanistic framework for modeling host-parasite$\backslash$ncoevolution using a nested modeling approach. The first step in this$\backslash$napproach is to construct a mechanistic model of the parasite population$\backslash$ndynamics within a host. The second step is to define an epidemiological$\backslash$nmodel which is used to derive the fitness functions for both the$\backslash$nhost and the parasite. The within-host model is then nested within$\backslash$nthe epidemiological model by linking the epidemiological parameters$\backslash$nsuch as the transmission rate of the infection or the additional$\backslash$nhost mortality rate to the dynamics of the within-host model. Nesting$\backslash$nthe within - host model into an epidemiological model allows us to$\backslash$nevaluate the fitness functions for each interactor which in turn$\backslash$nallows us to determine the coevolutionary dynamics of the system.$\backslash$nThis nested approach has the advantage over other approaches in that$\backslash$nmechanistic descriptions of the host-parasite biology are used to$\backslash$nderive, rather than impose, life-history trade-offs. We illustrate$\backslash$nthis framework by analysing a simple host-parasite system. In this$\backslash$nparticular system we find that the coevolutionary equilibrium is$\backslash$nalways stable and that host survivorship and parasite fitness vary$\backslash$ngreatly with the cost of the immune response and parasite growth.},
	author = {Gilchrist, Michael A and Sasaki, Akira},
	doi = {10.1006/yjtbi.3076},
	file = {:Users/Linh-phuong/Library/Application Support/Mendeley Desktop/Downloaded/Gilchrist, Sasakiz - 2002 - Modeling Host-Parasite Coevolution A Nested Approach Based on Mechanistic Models.pdf:pdf},
	journal = {J. Theor. Biol.},
	number = {3},
	pages = {289--308},
	title = {{{\{}M{\}}odeling {\{}H{\}}ost-{\{}P{\}}arasite {\{}C{\}}oevolution: {\{}A{\}} {\{}N{\}}ested {\{}A{\}}pproach {\{}B{\}}ased on {\{}M{\}}echanistic {\{}M{\}}odels}},
	url = {https://ac-els-cdn-com.insb.bib.cnrs.fr/S0022519302930766/1-s2.0-S0022519302930766-main.pdf?{\_}tid=95d96a49-1f71-4661-a8e2-bfd4adfc891b{\&}acdnat=1541782623{\_}db6d7171c765c1c9c00b49ff7e978465},
