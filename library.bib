	month = {aug},
	number = {3},
	pages = {376--379},
	publisher = {John Wiley {\&} Sons, Ltd (10.1111)},
	title = {{Protozoa as Hosts for Endosymbioses and the Conversion of Symbionts into Organelles,}},
	url = {http://doi.wiley.com/10.1111/j.1550-7408.1985.tb04031.x},
