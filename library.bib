@article{Ouedraogo2013,
	abstract = {Mosquito feeding assays play an important role in quantifying malaria transmission potential in epidemiological and clinical studies. At present, membrane feeding assays are incompletely standardised. This affects our understanding of the precision of the assay and its suitability for evaluating transmission-blocking interventions. Here, we present a detailed protocol for membrane feeding using Anopheles gambiae mosquitoes and naturally P. falciparum infected individuals.},
