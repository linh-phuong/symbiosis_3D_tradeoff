	journal = {ISME J.},
	number = {1},
	pages = {166--175},
	pmid = {27740611},
	publisher = {Nature Publishing Group},
	title = {{A quantitative analysis of the direct and indirect costs of nitrogen fixation: A model based on \textit{Azotobacter vinelandii}}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/27740611 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5315487},
