	journal = {Am. Nat.},
	keywords = {divergent selection,evolution,horizontal transmission,mutualism,vertical transmission,virulence},
	number = {5},
	pages = {613--627},
	pmid = {16224726},
	title = {{Diversification of transmission modes and the evolution of mutualism.}},
	url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=16224726{\&}retmode=ref{\&}cmd=prlinks{\%}5Cnpapers3://publication/doi/10.1086/491799},
	volume = {166},
