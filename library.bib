	keywords = {-3-hydroxy-3-methylhexanoic acid,-3-methyl-3-sulfanylhexan-1-ol,anopheles gambiae,axilla bacteria,human sweat,malaria mosquito,r,s},
	pages = {bjw106},
	pmid = {27789516},
	title = {{Behavioral response of the malaria mosquito, {\textless}i{\textgreater}Anopheles gambiae{\textless}/i{\textgreater} , to human sweat inoculated with axilla bacteria and to volatiles composing human axillary odor}},
