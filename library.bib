	pmid = {23282172},
	publisher = {Malaria Journal},
	title = {{Studying fitness cost of {\textless}i{\textgreater}Plasmodium falciparum{\textless}/i{\textgreater} infection in malaria vectors: validation of an appropriate negative control}},
