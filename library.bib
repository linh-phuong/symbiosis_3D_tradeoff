	keywords = {Mycorrhiza,growth depression,leaf expansion,partitioning of shoot dry matter,phosphorus},
	month = {mar},
	number = {3},
	pages = {449--462},
	publisher = {John Wiley {\&} Sons, Ltd (10.1111)},
	title = {{the Nature of Growth Depressions in Sunflower Caused By Vesicular--Arbuscular Mycorrhizal Infection}},
	url = {http://doi.wiley.com/10.1111/j.1469-8137.1985.tb03672.x},
	volume = {99},
