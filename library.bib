	keywords = {Coevolution,Eco-evolutionary feedbacks,Extinction,Major-gene discrete trait,Polygenic continuous trait,Red Queen dynamics},
	number = {1827},
	pmid = {27009218},
	title = {{Antagonistic coevolution between quantitative and mendelian traits}},
	url = {http://dx.doi.org/10.1098/rspb.2015.2926orviahttp://rspb.royalsocietypublishing.org.},
	volume = {283},
	year = {2016},
