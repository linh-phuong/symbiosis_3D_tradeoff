	journal = {Parasitology},
	keywords = {Basic reproduction ratio,Disease control,Epidemiology,Host-parasite community,Mathematical model,Parasitic helminth,Population dynamics,Virus},
	number = {13},
	pages = {1545--1560},
	pmid = {18070368},
	title = {{Worms and germs: The population dynamic consequences of microparasite-macroparasite co-infection}},
	url = {https://www.researchgate.net/publication/5778731},
	volume = {135},
	year = {2008},
