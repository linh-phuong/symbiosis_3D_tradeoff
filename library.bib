@article{Strotz2018,
	abstract = {The Red Queen hypothesis (RQH) is both familiar and murky, with a scope and range that has broadened beyond its original focus. Although originally developed in the palaeontological arena, it now encompasses many evolutionary theories that champion biotic interactions as significant mechanisms for evolutionary change. As such it de-emphasizes the important role of abiotic drivers in evolution, even though such a role is frequently posited to be pivotal. Concomitant with this shift in focus, several studies challenged the validity of the RQH and downplayed its propriety. Herein, we examine in detail the assumptions that underpin the RQH in the hopes of furthering conceptual understanding and promoting appropriate application of the hypothesis. We identify issues and inconsistencies with the assumptions of the RQH, and propose a redefinition where the Red Queen's reign is restricted to certain types of biotic interactions and evolutionary patterns occurring at the population level.},
	author = {Strotz, Luke C. and Sim{\~{o}}es, Marianna and Girard, Matthew G. and Breitkreuz, Laura and Kimmig, Julien and Lieberman, Bruce S.},
	doi = {10.1098/rsbl.2017.0734},
	file = {:Users/Linh-phuong/Library/Application Support/Mendeley Desktop/Downloaded/Strotz et al. - 2018 - Getting somewhere with the Red Queen chasing a biologically modern definition of the hypothesis.pdf:pdf},
	issn = {1744957X},
	journal = {Biol. Lett.},
	keywords = {Competition,Environment,Extinction,Macroevolution,Red Queen hypothesis},
	month = {may},
	number = {5},
	pages = {20170734},
	pmid = {29720444},
	publisher = {The Royal Society},
	title = {{Getting somewhere with the red queen: Chasing a biologically modern definition of the hypothesis}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/29720444 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6012711},
	volume = {14},
	year = {2018},
	bdsk-url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/29720444%20http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6012711},
	bdsk-url-2 = {https://doi.org/10.1098/rsbl.2017.0734}}

@techreport{Kisdi2006,
	abstract = {Background: A recently developed geometric method makes it possible to explore how the shape of a trade-off determines the outcome of adaptive evolution in any complex model and without committing to a particular functional form of the trade-off function. Aim: Extend the method to the co-evolution of two species. (The two species may be distantly related such as a predator and its prey, or may be closely related like two strategies produced by evolutionary branching.) Results: Thresholds of the local convexity of the trade-off functions are obtained that guarantee evolutionary and convergence stability when a given species pair is singular. In contrast to the single-species case, the condition for convergence is sufficient but not necessary. Criteria for evolutionary branching generalize from the single-species case. A cross-derivative of the invasion fitness determines whether evolutionary branching is possible; this quantity is independent of the trade-offs and if it is negative at a certain species pair, then the trade-offs can be chosen such that evolutionary branching occurs at this point. Worked example: A simple predator-prey model shows how these results can be used to identify trade-off functions such that evolution leads to an evolutionarily stable species pair or to evolutionary branching in either species.},
	author = {Kisdi, {\'{E}}va},
	booktitle = {Evol. Ecol. Res.},
	file = {:Users/Linh-phuong/Library/Application Support/Mendeley Desktop/Downloaded/Kisdi - 2006 - Trade-off geometries and the adaptive dynamics of two co-evolving species(2).pdf:pdf},
	issn = {15220613},
	keywords = {Adaptive dynamics,Co-evolution,Critical function analysis,Evolutionary branching,Evolutionary stable strategy,Geometric analysis,Predator-prey system,Trade-off},
	number = {6},
	pages = {959--973},
	title = {{Trade-off geometries and the adaptive dynamics of two co-evolving species}},
	url = {http://www.evolutionary-ecology.com/issues/v08n06/ccar2069.pdf},
	volume = {8},
	year = {2006},
	bdsk-url-1 = {http://www.evolutionary-ecology.com/issues/v08n06/ccar2069.pdf}}

