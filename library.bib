	issn = {00405809},
	journal = {Theor. Popul. Biol.},
	keywords = {Evolution of virulence,Host-parasite,Selection conflict,Viral dynamics,Within-host selection},
	number = {2},
	pages = {145--153},
	pmid = {16198387},
	title = {{Evolution of virulence: Interdependence, constraints, and selection using nested models}},
	url = {www.elsevier.com/locate/ytpbi},
	volume = {69},
