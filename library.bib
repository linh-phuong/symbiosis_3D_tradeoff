	journal = {Proc. Natl. Acad. Sci.},
	number = {46},
	pages = {18590--18595},
	pmid = {24167248},
	title = {{Polynucleobacter necessarius, a model for genome reduction in both free-living and symbiotic bacteria}},
	url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1316687110},
	volume = {110},
